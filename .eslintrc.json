{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:vue/vue3-raccomanded", "prettier"],
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["vue", "prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "vue/no-multiple-template-root": "off"
  }
}
